{
    "style": {
        "position": "absolute",
        "z-index": 31
    },
    "background": {
        "opacity": 0
    },
    "fps_limit": 30,
    "interactivity": {
        "detectsOn": "canvas",
        "events": {
            "onClick": {
                "enable": true,
                "mode": "push"
            },
            "onHover": {
                "enable": true,
                "mode": "connect"
            }
        }
    },
    "particles": {
        "color": {
            "value": "#ffffff"
        },
        "links": {
            "color": {
                "value": "#ffffff"
            },
            "enable": true,
            "opacity": 0.4
        },
        "move": {
            "enable": true,
            "outModes": "out",
            "speed": 2
        },
        "number": {
            "density": {
                "enable": true
            },
            "value": 40
        },
        "opacity": {
            "value": 0.5
        },
        "size": {
            "value": 3,
            "random": true
        }
    },
    "responsive": "canvas"
}
